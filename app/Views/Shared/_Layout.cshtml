
<!DOCTYPE html>

<html>
<head>
  <title>@ViewData["Title"]</title>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <link rel="stylesheet" href="~/assets/css/main.css" />
</head>
<body class="subpage">

  <header id="header">
    <div class="inner">
      <a asp-controller="Home" asp-action="Index" class="logo"><strong>SberSeasons</strong></a>
      <nav id="nav">
        <a asp-controller="Home" asp-action="Index">Главная</a>
        <a asp-controller="Home" asp-action="About">Задание</a>
        <a asp-controller="Home" asp-action="Contact">Контакты</a>
      </nav>
      <a href="#navPanel" class="navPanelToggle"><span class="fa fa-bars"></span></a>
    </div>
  </header>

  @RenderBody()

  <script src="~/assets/js/jquery.min.js"></script>
  <script src="~/assets/js/skel.min.js"></script>
  <script src="~/assets/js/util.js"></script>
  <script src="~/assets/js/main.js"></script>
</body>
</html>
